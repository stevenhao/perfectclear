{
  "name": "tetris2",
  "version": "1.0.0",
  "description": "Perfect Clear Tetris Game",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:frontend": "vite build",
    "dev:vite": "vite",
    "start": "make && node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "start:coffee": "coffee index.coffee 4445",
    "start:backend": "./server.o 4445",
    "dev:frontend": "nodemon --ext coffee,js,html,css --watch index.coffee --watch public --exec \"coffee index.coffee 4445\"",
    "dev:backend": "./server.o 4445",
    "dev:coffee": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:modern": "concurrently \"npm run dev:backend\" \"npm run dev:vite\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/router": "^0.0.1-beta.53",
    "@types/express": "4.17.21",
    "body-parser": "^1.15.0",
    "child_process": "^1.0.2",
    "express": "^4.13.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.9",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.4.1",
    "coffeescript": "^1.12.7",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
